{
  "$schema": "http://json.schemastore.org/template",
  "author": "KazoHome",
  "classifications": ["Home Assistant", "Card", "Blazor", "WebAssembly"],
  "identity": "KazoHome.Templates.Card",
  "name": "KazoHome Dashboard Card",
  "shortName": "kazohome-card",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "KazoHomeCard",
  "preferNameDirectory": true,
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net10.0",
          "description": "Target net10.0"
        }
      ],
      "defaultValue": "net10.0"
    },
    "CardName": {
      "type": "parameter",
      "dataType": "string",
      "defaultValue": "My Custom Card",
      "description": "The display name of the card"
    }
  }
}
