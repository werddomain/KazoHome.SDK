{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "KazoHomeAddon": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    },
    "Docker": {
      "commandName": "Docker"
    },
    "Remote Debug (vsdbg)": {
      "commandName": "Executable",
      "executablePath": "ssh",
      "commandLineArgs": "-t homeassistant \"cd /addon && vsdbg --interpreter=vscode\"",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    }
  }
}
